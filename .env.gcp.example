# Cloud SQL Configuration for Tactical Touchdown
# Copy this to .env.gcp and fill in your Cloud SQL connection string

# Google Cloud Project
GCP_PROJECT_ID=your-project-id

# Cloud SQL Connection
# Format: postgresql://user:password@cloudsql-instance/database
# Or use Cloud SQL Proxy: /cloudsql/PROJECT:REGION:INSTANCE
DATABASE_URL=postgresql://user:password@localhost:5432/tactical_touchdown

# Environment
NODE_ENV=production
PORT=8080

# Flask backend
FLASK_ENV=production
